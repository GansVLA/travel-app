<?xml version='1.0' encoding='utf-8'?>
<widget id="com.turismoizucar.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Turismo Iz√∫car</name>
    <description>
        A basic Framework7 template for PhoneGap.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <access launch-external="yes" origin="tel:*" />
    <access launch-external="yes" origin="mailto:*" />
    <access launch-external="yes" origin="geo:*" />
    <acces launch-external="yes" origin="https://www.facebook.com/*" />
    <access launch-external="yes" origin="https://*" />
    <access launch-external="yes" origin="http://*" />
    <access launch-external="yes" origin="fb://profile/*" />
    <access launch-external="yes" origin="fb://*" />
    <access launch-external="yes" origin="whatsapp://*" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
